{

    "terminal.integrated.profiles.windows": {
        "PowerShell": {
          "source": "PowerShell",
          "icon": "terminal-powershell"
        },
        "Command Prompt": {
          "path": [
            "${env:windir}\\Sysnative\\cmd.exe",
            "${env:windir}\\System32\\cmd.exe"
          ],
          "args": [],
          "icon": "terminal-cmd"
        },
        "Git Bash": {
          "source": "Git Bash"
        },
        "MSYS2": {
          "path": "C:\\msys64\\usr\\bin\\bash.exe",
          "args": [
            "--login",
            "-i"
          ],
          "env": {
            "MSYSTEM": "UCRT64",
            "CHERE_INVOKING": "1"
          }
        }
      },
      "files.associations": {
        "stdio.h": "c",
        "defines.h": "c"
      }
    
}